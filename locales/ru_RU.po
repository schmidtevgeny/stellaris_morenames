msgid "$job_eo_scrapper$"
msgstr "$job_eo_scrapper$"

#: update.py:3903
msgid "$job_eo_scrapper_desc$"
msgstr "$job_eo_scrapper_desc$"

#: update.py:3906
msgid "$job_eo_scrapper_effect_desc$"
msgstr "$job_eo_scrapper_effect_desc$"

#: update.py:3909
msgid "$job_eo_scrapper_plural$"
msgstr "$job_eo_scrapper_plural$"

